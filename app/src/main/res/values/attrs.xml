<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconView">
        <attr name="text" format="string" />
        <attr name="iconDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="BankIcon">
        <attr name="textBank" format="string" />
        <attr name="bankIcon" format="reference" />
        <attr name="checked" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="textCheckedColor" format="color" />
        <attr name="boldCheckedText" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DebitTypeView">
        <attr name="debitName" format="string" />
        <attr name="imageDr" format="string" />
        <attr name="datetimeDr" format="string" />
        <attr name="methodDr" format="string" />
        <attr name="amountDr" format="float" />
    </declare-styleable>

    <declare-styleable name="CreditTypeView">
        <attr name="creditName" format="string" />
        <attr name="imageCr" format="string" />
        <attr name="datetimeCr" format="string" />
        <attr name="methodCr" format="string" />
        <attr name="amountCr" format="float" />
    </declare-styleable>
</resources>